{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework", "api"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "cknow/laravel-money": "^8.4",
        "farayaz/laravel-spy": "^1.3",
        "laravel/fortify": "^1.32",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.13",
        "laravel/reverb": "^1.6",
        "laravel/sanctum": "^4.2",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.10.1",
        "pinkary-project/type-guard": "^0.1.0",
        "resend/resend-php": "^0.14.0",
        "spatie/laravel-medialibrary": "^11.17",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-query-builder": "^6.3",
        "symfony/mailer": "^7.2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.2",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/boost": "*",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "laravel/telescope": "^5.15",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer update",
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate:fresh --seed",
            "@cache:clear",
            "@php artisan boost:update"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74,#34d399\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan reverb:start\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,reverb,logs,vite --kill-others"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ],
        "cache:clear": [
            "@php artisan config:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan event:clear",
            "@php artisan cache:clear",
            "@php artisan optimize:clear"
        ],
        "pre:test": [
            "Composer\\Config::disableProcessTimeout",
            "@format",
            "@cache:clear"
        ],
        "test": [
            "@pre:test",
            "./vendor/bin/pest --parallel"
        ],
        "test:unit": [
            "@pre:test",
            "./vendor/bin/pest --parallel --filter=Unit"
        ],
        "test:feature": [
            "@pre:test",
            "./vendor/bin/pest --parallel --filter=Feature"
        ],
        "test:coverage": [
            "@pre:test",
            "XDEBUG_MODE=coverage ./vendor/bin/pest --coverage --coverage-html=coverage"
        ],
        "test:type-coverage": [
            "@pre:test",
            "./vendor/bin/pest --type-coverage"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse -c phpstan.neon --ansi --memory-limit=1024M"
        ],
        "format": [
            "./vendor/bin/pint --ansi",
            "./vendor/bin/rector --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
