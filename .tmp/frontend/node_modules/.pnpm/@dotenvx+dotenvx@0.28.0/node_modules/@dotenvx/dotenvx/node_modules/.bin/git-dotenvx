#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules/@dotenvx/dotenvx/src/cli/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules/@dotenvx/dotenvx/src/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules/@dotenvx/dotenvx/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules/@dotenvx/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules/@dotenvx/dotenvx/src/cli/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules/@dotenvx/dotenvx/src/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules/@dotenvx/dotenvx/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules/@dotenvx/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/@dotenvx+dotenvx@0.28.0/node_modules:/Users/yusufkaracaburun/Sites/localhost/laravel/demo/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../src/cli/dotenvx.js" "$@"
else
  exec node  "$basedir/../../src/cli/dotenvx.js" "$@"
fi
