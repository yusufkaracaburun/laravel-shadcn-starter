{
  "name": "frontend",
  "type": "module",
  "version": "1.0.1",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "description": "Frontend application for the project",
  "author": "yusufkaracaburun",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue-router",
    "vite",
    "typescript",
    "tailwindcss",
    "shadcn-vue",
    "tanstack-vue-query",
    "tanstack-table",
    "eslint",
    "pinia",
    "pnpm"
  ],
  "scripts": {
    "postinstall": "simple-git-hooks",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:component": "vitest run tests/components",
    "test:api": "playwright test --project=api",
    "test:pact": "echo 'Pact tests komen later'",
    "test:e2e": "playwright test --project=e2e",
    "test:e2e:failed": "playwright test --last-failed",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test:component && npm run test:api && npm run test:e2e",
    "release": "npx changelogen@latest --release"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@internationalized/date": "^3.10.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/vue-query": "^5.92.0",
    "@tanstack/vue-table": "^8.21.3",
    "@unovis/ts": "^1.6.2",
    "@unovis/vue": "^1.6.2",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.1.0",
    "@vueuse/integrations": "^14.1.0",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-vue": "^8.6.0",
    "laravel-echo": "^2.2.6",
    "lucide-vue-next": "^0.555.0",
    "motion-v": "1.7.4",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "pusher-js": "^8.4.0",
    "reka-ui": "^2.6.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "universal-cookie": "^8.0.1",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vue-input-otp": "^0.3.2",
    "vue-router": "^4.6.3",
    "vue-sonner": "^2.0.9",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.3.0",
    "@dotenvx/dotenvx": "^1.51.1",
    "@faker-js/faker": "^10.1.0",
    "@iconify-json/simple-icons": "^1.2.61",
    "@iconify/vue": "^5.0.0",
    "@playwright/test": "^1.57.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/vue-query-devtools": "^6.1.2",
    "@types/node": "^24.10.1",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "simple-git-hooks": "^2.13.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.18.0",
    "vite": "^7.2.6",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "7",
        "zod": "4"
      }
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "maplibre-gl",
      "simple-git-hooks",
      "vue-demi"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,md,css,html}": [
      "prettier --write"
    ],
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  }
}
